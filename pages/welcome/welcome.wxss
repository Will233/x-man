/* pages/welcome/welcome.wxss */
.app-info {
  width: 100%;
  text-align: center;
  padding-top:50px;
  display:block;
}
.icon-wrap {
  margin: 0 auto;
  display: block;
  width: 200rpx;
  height: 200rpx;
  background-color: #ccc;
  background: url('data:image/png;base64,') no-repeat;
  background-size: cover;
}

.app-name {
  display: block;
  font-size: 40rpx;
  margin: 0 auto; 
  margin-top: 20rpx; 
}
.opt-wrap {
  display: block;
  padding-top: 100rpx;
}
.opt-wrap button{
  width: 300rpx;
  height: 80rpx;
  font-size: 36rpx;
  line-height: 80rpx;
  border-radius: 80rpx;
  /*color: #fff;*/
  color: #fdb64a;
  background: #fff;
  border: solid 1rpx #fdb64a;
}